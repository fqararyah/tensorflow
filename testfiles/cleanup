rm /home/3dd/whl_files/*
pip uninstall tensorflow
rm train_results/*

echo "Removed old installation\n"

cd /home/3dd/downloads/tensorflow-source/tensorflow
bazel build --config=opt --config=cuda //tensorflow/tools/pip_package:build_pip_package
bazel-bin/tensorflow/tools/pip_package/build_pip_package /home/3dd/whl_files

echo "Created new .whl file\n"

pip install /home/3dd/whl_files/*

echo "Installed new custom TF build!\n"

cd /home/3dd/

